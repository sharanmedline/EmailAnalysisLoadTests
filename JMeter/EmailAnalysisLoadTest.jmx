        <elementProp name="ThreadGroup.scheduler" elementType="elementProp" elementType="ConstantThroughputTimer" guiclass="ConstantThroughputTimerGui" testclass="ConstantThroughputTimer" testname="constant Throughput Timer" enabled="false">
